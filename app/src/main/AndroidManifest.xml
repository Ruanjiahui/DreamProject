        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.WebViewActivity" />
        <activity android:name=".Activity.Me" />
        <activity android:name=".Activity.Personal" />
        <activity android:name=".Activity.Setting" />
        <activity android:name=".Activity.Product" />
        <activity android:name=".Activity.Personal_Information" />
        <activity android:name=".Activity.ProductList" />
        <activity android:name=".Activity.ContactList" />
        <activity android:name=".Activity.Suggest" />
        <activity android:name=".Activity.AboutMe" />
        <activity android:name=".Activity.People" />
        <activity android:name=".Activity.Chat" />
        <activity android:name=".Activity.Comment" />
        <activity android:name=".Other" />
        <activity android:name=".Activity.Comment_List" />
        <activity android:name=".Activity.Load" />
        <activity android:name=".Activity.Register" />
        <activity android:name=".Activity.Release" />
        <activity android:name=".SystemImage"/>
        <activity android:name=".Activity.Show"/>
    </application>


</manifest>
